{
  "name": "coco-dev Environment",
  "image": "jamieleecho/coco-dev:latest",
  "containerEnv": {
      "VIRTUAL_ENV": "/home/vscode/.venv",
      "PATH": "/home/vscode/.venv/bin:/root/venv/bin:${localEnv:PATH}"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "blairleduc.6x09-assembly",
        "llvm-vs-code-extensions.vscode-clangd",
        "ms-vscode.makefile-tools",
        "ms-azuretools.vscode-docker",
        "ms-python.debugpy",
        "ms-python.mypy-type-checker",
        "ms-python.python"
      ]
    }
  },
  "forwardPorts": [3000],
  "postCreateCommand": "./.devcontainer/post-create-command.sh"
}
